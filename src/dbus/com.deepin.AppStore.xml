    </method>

    <method name="OpenApp">
        <arg direction="in" type="s"/>
    </method>

    <method name="ShowAppDetail">
        <arg direction="in" type="s"/>
    </method>

    <method name="GetAppMetadata">
        <arg direction="in" type="s"/>
